{
  "name": "Skillshare Player Control",
  "description": "Basic keyboard controls for Skillshare's player.",
  "manifest_version": 2,
  "version": "1.2.1",
  "author": "avi12",
  "content_scripts": [
    {
      "matches": [
        "https://www.skillshare.com/classes/*"
      ],
      "js": [
        "build/skillshare-content-script-initialize.js"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "19": "images/icon-19.png",
    "24": "images/icon-24.png",
    "32": "images/icon-32.png",
    "38": "images/icon-38.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options/options.html"
  },
  "applications": {
    "gecko": {
      "id": "Skillshare_player_controller@gmail.com"
    }
  }
}